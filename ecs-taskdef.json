{
  "family": "budgetbot-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "REPLACE_EXEC_ROLE_ARN",
  "taskRoleArn": "REPLACE_TASK_ROLE_ARN",
  "containerDefinitions": [
    {
      "name": "budgetbot",
      "image": "REPLACE_IMAGE",
      "essential": true,
      "portMappings": [
        { "containerPort": 8080, "protocol": "tcp" }
      ]
    }
  ]
}